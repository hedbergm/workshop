<section class="card">
  <h1>Logg inn</h1>
  <% if (error) { %><ul class="flash"><li><%= error %></li></ul><% } %>
  <form method="post">
    <label>Brukernavn
      <input type="text" name="username" required>
    </label>
    <label>Passord
      <input type="password" name="password" required>
    </label>
    <button type="submit" class="btn">Logg inn</button>
  </form>
</section>
